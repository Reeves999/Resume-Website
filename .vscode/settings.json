{
  "editor.fontSize": 12,
  "editor.wordWrap": "bounded",
  "editor.codeLensFontSize": 12,
  "editor.columnSelection": true,
  "editor.comments.ignoreEmptyLines": false,
  "editor.defaultFormatter": "VisualStudioExptTeam.vscodeintellicode",
  "editor.definitionLinkOpensInPeek": true,
  "editor.foldingImportsByDefault": true,
  "editor.gotoLocation.alternativeDefinitionCommand": "editor.action.revealDefinition",
  "editor.gotoLocation.alternativeImplementationCommand": "editor.action.peekImplementation",
  "editor.gotoLocation.alternativeReferenceCommand": "editor.action.peekImplementation",
  "editor.gotoLocation.alternativeDeclarationCommand": "editor.action.revealDefinitionAside",
  "editor.guides.bracketPairs": "active",
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlayHints.fontSize": 1,
  "editor.inlayHints.padding": true,
  "[javascript]": {
    "editor.defaultFormatter": "svipas.prettier-plus"
  },
  "MicroPython.executeButton": [
    {
      "text": "â–¶",
      "tooltip": "Run",
      "alignment": "left",
      "command": "extension.executeFile",
      "priority": 3.5
    }
  ],
  "MicroPython.syncButton": [
    {
      "text": "$(sync)",
      "tooltip": "sync",
      "alignment": "left",
      "command": "extension.execute",
      "priority": 4
    }
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "livePreview.defaultPreviewPath": "c:\\Users\\Daniel A Reeves\\Documents\\GitHub\\Resume\\index.html",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:Resume}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  }
}
